  },
  "homepage": "https://github.com/UofT-UTSC-CS-sandbox/project-docker_demons#readme",
  "dependencies": {
    "body-parser": "^1.20.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "next": "^13.2.3",
    "pg": "^8.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sequelize": "^6.29.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.6",
    "@types/pg": "^8.6.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "concurrently": "^7.6.0",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "nodemon": "^2.0.21",
    "typescript": "^4.9.5"
  }
}
